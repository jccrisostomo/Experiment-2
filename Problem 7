#include <conio.h>
#include <iostream>
using namespace std;

int main()
{
        int x, y, sum; 
        cout << "Enter first number" << endl;
        cin >> x;
        cout << "Enter last number" << endl;
        cin >> y;
        
        for ( int number = x; number <= y; number += x )
        { 
           sum += number; 
        }
        cout << "The summation from "<<x<<" to "<<y<<" is " << sum << endl; 
        return 0;
}
