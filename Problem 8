#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
        float num;

        cout << "Enter a number" << endl;
        cin >> num;

        for (float i = 1; i <= 10; ++i) 
        {
           cout << num << " * " << i << " = " << num * i << endl;
        }
        return 0;
}
